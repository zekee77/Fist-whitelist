do local request=function(options) return game:HttpGet(options.Url);end;local hwid=game:GetService("RbxAnalyticsService"):GetClientId();local whitelist=request({Url="https://raw.githubusercontent.com/zekee77/Fist-whitelist/main/hwidlist.json"});local valid=false;for _,id in ipairs(game:GetService("HttpService"):JSONDecode(whitelist)) do if (id==hwid) then valid=true;break;end end if  not valid then game.Players.LocalPlayer:Kick("Unauthorized HWID.");return;end local player=game.Players.LocalPlayer;local runService=game:GetService("RunService");local screenGui=Instance.new("ScreenGui",game.CoreGui);local frame=Instance.new("Frame");frame.Size=UDim2.new(0,220,0,300);frame.Position=UDim2.new(0.4,0,0.3,0);frame.BackgroundColor3=Color3.fromRGB(40,40,40);frame.BorderSizePixel=2;frame.Active=true;frame.Draggable=true;frame.Parent=screenGui;local textBox=Instance.new("TextBox");textBox.Size=UDim2.new(0,200,0,40);textBox.Position=UDim2.new(0.5, -100,0.05,0);textBox.PlaceholderText="Enter Player Name";textBox.BackgroundColor3=Color3.fromRGB(50,50,50);textBox.TextColor3=Color3.new(1,1,1);textBox.Font=Enum.Font.SourceSansBold;textBox.TextSize=18;textBox.Parent=frame;local studsLabel=Instance.new("TextLabel");studsLabel.Size=UDim2.new(0,200,0,40);studsLabel.Position=UDim2.new(0.5, -100,0.2,0);studsLabel.Text="Studs: 3";studsLabel.BackgroundColor3=Color3.fromRGB(50,50,50);studsLabel.TextColor3=Color3.new(1,1,1);studsLabel.Font=Enum.Font.SourceSansBold;studsLabel.TextSize=18;studsLabel.Parent=frame;local studsInput=Instance.new("TextBox");studsInput.Size=UDim2.new(0,200,0,40);studsInput.Position=UDim2.new(0.5, -100,0.35,0);studsInput.Text="3";studsInput.PlaceholderText="Set Distance";studsInput.BackgroundColor3=Color3.fromRGB(50,50,50);studsInput.TextColor3=Color3.new(1,1,1);studsInput.Font=Enum.Font.SourceSansBold;studsInput.TextSize=18;studsInput.Parent=frame;local toggleButton=Instance.new("TextButton");toggleButton.Size=UDim2.new(0,200,0,40);toggleButton.Position=UDim2.new(0.5, -100,0.65,0);toggleButton.Text="Start Bringing";toggleButton.BackgroundColor3=Color3.fromRGB(0,170,255);toggleButton.TextColor3=Color3.new(1,1,1);toggleButton.Font=Enum.Font.SourceSansBold;toggleButton.TextSize=18;toggleButton.Parent=frame;local isBringing=false;local humanoidRootPart;local originalPositions={};local function makePartsVisible(targetCharacter) for _,part in ipairs(targetCharacter:GetDescendants()) do if part:IsA("BasePart") then part.Transparency=0;part.Material=Enum.Material.SmoothPlastic;elseif (part:IsA("Decal") or part:IsA("Texture")) then part.Transparency=0;end end end local function storeOriginalPosition(targetCharacter) local targetHRP=targetCharacter:FindFirstChild("HumanoidRootPart");if targetHRP then originalPositions[targetCharacter.Name]=targetHRP.Position;end end local function bringPlayer(distance) while isBringing do if humanoidRootPart then local targetName=textBox.Text;local targetPlayer=nil;for _,otherPlayer in ipairs(game.Players:GetPlayers()) do if ((otherPlayer.Name:lower()==targetName:lower()) or (otherPlayer.DisplayName:lower()==targetName:lower())) then targetPlayer=otherPlayer;break;end end if targetPlayer then local targetCharacter=targetPlayer.Character;if targetCharacter then local otherHRP=targetCharacter:FindFirstChild("HumanoidRootPart");if otherHRP then otherHRP.Position=humanoidRootPart.Position + Vector3.new(distance,0,0) ;makePartsVisible(targetCharacter);end end end end task.wait(0.1);end end local function returnToOriginalPosition() for _,otherPlayer in ipairs(game.Players:GetPlayers()) do if originalPositions[otherPlayer.Name] then local targetCharacter=otherPlayer.Character;if targetCharacter then local targetHRP=targetCharacter:FindFirstChild("HumanoidRootPart");if targetHRP then targetHRP.Position=originalPositions[otherPlayer.Name];end end end end end toggleButton.MouseButton1Click:Connect(function() isBringing= not isBringing;toggleButton.Text=(isBringing and "Stop Bringing") or "Start Bringing" ;toggleButton.BackgroundColor3=(isBringing and Color3.fromRGB(255,0,0)) or Color3.fromRGB(0,170,255) ;local distance=tonumber(studsInput.Text) or 3 ;if isBringing then originalPositions={};task.spawn(function() bringPlayer(distance);end);else returnToOriginalPosition();end end);local function onCharacterAdded(character) humanoidRootPart=character:WaitForChild("HumanoidRootPart");end player.CharacterAdded:Connect(onCharacterAdded);if player.Character then onCharacterAdded(player.Character);end end
